<!doctype html>
<html>
<head>
	<meta charset="utf-8">
	<link href='https://white-label-elements.herokuapp.com/utils.css' rel='stylesheet'>
	<link href='https://white-label-elements.herokuapp.com/banner/main.css' rel='stylesheet'>
	<link href='https://white-label-elements.herokuapp.com/layout/main.css' rel='stylesheet'>
	<link href='https://white-label-elements.herokuapp.com/form/main.css' rel='stylesheet'>
	<link href='https://white-label-elements.herokuapp.com/search-item/main.css' rel='stylesheet'>
	<link href='https://white-label-elements.herokuapp.com/pagination/main.css' rel='stylesheet'>
	<link href='https://white-label-elements.herokuapp.com/footer/main.css' rel='stylesheet'>
	<link href='https://white-label-elements.herokuapp.com/selection-box.css' rel='stylesheet'>
	<link href='https://white-label-elements.herokuapp.com/loading-panel/main.css' rel='stylesheet'>
	<link href='https://fonts.googleapis.com/css?family=Bangers' rel='stylesheet' type='text/css'>
	<link href='https://fonts.googleapis.com/css?family=Ubuntu' rel='stylesheet' type='text/css'>
	<link href='https://fonts.googleapis.com/css?family=Ubuntu' rel='stylesheet' type='text/css'>
	<link href='/main.css' rel='stylesheet' type='text/css'>

	<script>
		window.config = {
			API_URL : '<% this.apiUrl %>'
		};
	</script>
</head>
<body>

	<div id='app'></div>
  <div><% this.body %></div>

	<div class='spinner'>
		<img src='https://white-label-elements.herokuapp.com/arrow.svg'/>
	</div>

  <% if(this.isDev) { %>
		<script src='/system.js'></script>
		<script src='/config.js'></script>
		<script>
		  System.import('/js/main.js');
		</script>
  <% } else { %>
		<script src='/app.js'></script>
  <% } %>

</body>
</html>
