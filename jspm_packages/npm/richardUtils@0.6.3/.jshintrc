{
	"browser" : true,
	"curly" : true,
	"eqeqeq" : false,
	"esversion" : 5,
	"forin" : true,
	"freeze" : true,
	"funcscope" : false,
	"futurehostile" : true,
	"jasmine" : true,
	"globals" : {
		"console" : true,
